version 41
__lua__
-- init called once at startup
function _init()
	--clear screen
	cls(0)
	
	harry=40
	mimi=100
end

-- drawn called when new frame
-- gets drawn at 30 fps
function _draw()
	cls(0)
	print(harry,mimi,harry)
	circ(64,64,mimi)
		circ(64,64,harry)
	spr(3,harry,64)
end

-- update is for gameplay logic
-- and math at hard 30 fps
function _update()
	harry=harry+1
	mimi=mimi-1
end
